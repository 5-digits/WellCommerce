{
    "name": "wellcommerce/wellcommerce",
    "license": "MIT",
    "minimum-stability": "dev",
    "description": "WellCommerce Open-Source edition",
    "keywords": ["symfony","e-commerce","open-source"],
    "authors": [
        {
            "name": "Adam Piotrowski",
            "email": "adam@wellcommerce.org"
        },
        {
            "name": "WellCommerce Developers",
            "email": "developers@wellcommerce.org"
        }
    ],
    "support": {
        "email": "support@wellcommerce.org",
        "issues": "https://github.com/WellCommerce/WellCommerce/issues",
        "source": "https://github.com/WellCommerce/WellCommerce/"
    },
    "autoload": {
        "psr-4": {
            "WellCommerce\\Bundle\\AdminBundle\\": "src/WellCommerce/Bundle/AdminBundle",
            "WellCommerce\\Bundle\\ApiBundle\\": "src/WellCommerce/Bundle/ApiBundle",
            "WellCommerce\\Bundle\\AppBundle\\": "src/WellCommerce/Bundle/AppBundle",
            "WellCommerce\\Bundle\\AttributeBundle\\": "src/WellCommerce/Bundle/AttributeBundle",
            "WellCommerce\\Bundle\\AvailabilityBundle\\": "src/WellCommerce/Bundle/AvailabilityBundle",
            "WellCommerce\\Bundle\\CartBundle\\": "src/WellCommerce/Bundle/CartBundle",
            "WellCommerce\\Bundle\\CategoryBundle\\": "src/WellCommerce/Bundle/CategoryBundle",
            "WellCommerce\\Bundle\\ClientBundle\\": "src/WellCommerce/Bundle/ClientBundle",
            "WellCommerce\\Bundle\\CompanyBundle\\": "src/WellCommerce/Bundle/CompanyBundle",
            "WellCommerce\\Bundle\\ContactBundle\\": "src/WellCommerce/Bundle/ContactBundle",
            "WellCommerce\\Bundle\\CoreBundle\\": "src/WellCommerce/Bundle/CoreBundle",
            "WellCommerce\\Bundle\\CountryBundle\\": "src/WellCommerce/Bundle/CountryBundle",
            "WellCommerce\\Bundle\\CouponBundle\\": "src/WellCommerce/Bundle/CouponBundle",
            "WellCommerce\\Bundle\\CurrencyBundle\\": "src/WellCommerce/Bundle/CurrencyBundle",
            "WellCommerce\\Bundle\\DelivererBundle\\": "src/WellCommerce/Bundle/DelivererBundle",
            "WellCommerce\\Bundle\\DictionaryBundle\\": "src/WellCommerce/Bundle/DictionaryBundle",
            "WellCommerce\\Bundle\\DistributionBundle\\": "src/WellCommerce/Bundle/DistributionBundle",
            "WellCommerce\\Bundle\\DoctrineBundle\\": "src/WellCommerce/Bundle/DoctrineBundle",
            "WellCommerce\\Bundle\\LayeredNavigationBundle\\": "src/WellCommerce/Bundle/LayeredNavigationBundle",
            "WellCommerce\\Bundle\\LayoutBundle\\": "src/WellCommerce/Bundle/LayoutBundle",
            "WellCommerce\\Bundle\\LocaleBundle\\": "src/WellCommerce/Bundle/LocaleBundle",
            "WellCommerce\\Bundle\\MediaBundle\\": "src/WellCommerce/Bundle/MediaBundle",
            "WellCommerce\\Bundle\\NewsBundle\\": "src/WellCommerce/Bundle/NewsBundle",
            "WellCommerce\\Bundle\\OAuthBundle\\": "src/WellCommerce/Bundle/OAuthBundle",
            "WellCommerce\\Bundle\\OrderBundle\\": "src/WellCommerce/Bundle/OrderBundle",
            "WellCommerce\\Bundle\\PageBundle\\": "src/WellCommerce/Bundle/PageBundle",
            "WellCommerce\\Bundle\\PaymentBundle\\": "src/WellCommerce/Bundle/PaymentBundle",
            "WellCommerce\\Bundle\\ProducerBundle\\": "src/WellCommerce/Bundle/ProducerBundle",
            "WellCommerce\\Bundle\\ProductBundle\\": "src/WellCommerce/Bundle/ProductBundle",
            "WellCommerce\\Bundle\\ProductStatusBundle\\": "src/WellCommerce/Bundle/ProductStatusBundle",
            "WellCommerce\\Bundle\\ReportBundle\\": "src/WellCommerce/Bundle/ReportBundle",
            "WellCommerce\\Bundle\\ReviewBundle\\": "src/WellCommerce/Bundle/ReviewBundle",
            "WellCommerce\\Bundle\\RoutingBundle\\": "src/WellCommerce/Bundle/RoutingBundle",
            "WellCommerce\\Bundle\\SearchBundle\\": "src/WellCommerce/Bundle/SearchBundle",
            "WellCommerce\\Bundle\\ShippingBundle\\": "src/WellCommerce/Bundle/ShippingBundle",
            "WellCommerce\\Bundle\\ShopBundle\\": "src/WellCommerce/Bundle/ShopBundle",
            "WellCommerce\\Bundle\\TaxBundle\\": "src/WellCommerce/Bundle/TaxBundle",
            "WellCommerce\\Bundle\\ThemeBundle\\": "src/WellCommerce/Bundle/ThemeBundle",
            "WellCommerce\\Bundle\\UnitBundle\\": "src/WellCommerce/Bundle/UnitBundle",
            "WellCommerce\\Bundle\\WishlistBundle\\": "src/WellCommerce/Bundle/WishlistBundle",
            "WellCommerce\\Component\\Collections\\": "src/WellCommerce/Component/Collections",
            "WellCommerce\\Component\\DataGrid\\": "src/WellCommerce/Component/DataGrid",
            "WellCommerce\\Component\\DataSet\\": "src/WellCommerce/Component/DataSet",
            "WellCommerce\\Component\\Form\\": "src/WellCommerce/Component/Form"
        },
        "files": [ "app/AppKernel.php" ]
    },
    "require": {
        "php": "^7.0",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-exif": "*",
        "symfony/symfony": "^3.0",
        "doctrine/orm": "2.5.4",
        "twig/extensions": "1.3.x-dev",
        "sensio/distribution-bundle": "^5.0@dev",
        "sensio/framework-extra-bundle": "^3.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "knplabs/doctrine-behaviors": "dev-master",
        "fzaninotto/faker": "1.5.0",
        "friendsofsymfony/jsrouting-bundle": "^2.0@dev",
        "willdurand/js-translation-bundle": "^2.6@dev",
        "liip/imagine-bundle": "^1.3",
        "symfony-cmf/routing": "^1.3",
        "beberlei/DoctrineExtensions": "1.0.*@dev",
        "symfony/assetic-bundle": "^2.7@dev",
        "symfony/monolog-bundle": "^2.7@dev",
        "symfony/swiftmailer-bundle": "^2.3@dev",
        "doctrine/doctrine-migrations-bundle": "^1.0@dev",
        "twig/twig": "^1.2@dev",
        "wellcommerce/composer-plugin": "^1.0@dev",
        "react/child-process": "0.5.*@dev",
        "knplabs/packagist-api": "1.*@dev",
        "mcuelenaere/composer-revision-plugin": "0.1.*",
        "devristo/phpws": "dev-master",
        "nesbot/carbon": "dev-master",
        "zendframework/zendsearch": "dev-master",
        "zendframework/zend-json": "^2.6@dev",
        "mmoreram/simple-doctrine-mapping": "^1.1",
        "league/oauth2-facebook": "~1.0",
        "wingu/code-generator": "^1.0",
        "sensio/generator-bundle": "^3.0@dev",
        "doctrine/doctrine-fixtures-bundle": "^2.2",
        "cache/adapter-bundle": "0.3.2",
        "cache/apcu-adapter": "0.2.1",
        "cache/filesystem-adapter": "0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit":  "^5.0@dev"
    },
    "scripts": {
        "pre-install-cmd": [
            "WellCommerce\\Bundle\\DistributionBundle\\Composer\\HerokuEnvironment::populateEnvironment"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "build-bootstrap": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "env-map": {
                "database_name":        "SYMFONY_DATABASE_NAME",
                "database_user":        "SYMFONY_DATABASE_USER",
                "database_host":        "SYMFONY_DATABASE_HOST",
                "database_password":    "SYMFONY_DATABASE_PASSWORD",
                "prod_log_path":        "SYMFONY_PROD_LOG_PATH"
            }
        },
        "branch-alias": {
            "dev-master": "0.7.x-dev"
        }
    }
}
