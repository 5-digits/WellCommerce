{
  "name": "wellcommerce/wellcommerce",
  "license": "MIT",
  "minimum-stability": "dev",
  "description": "WellCommerce Open-Source edition",
  "keywords": [
    "symfony",
    "e-commerce",
    "open-source"
  ],
  "authors": [
    {
      "name": "Adam Piotrowski",
      "email": "adam@wellcommerce.org"
    },
    {
      "name": "WellCommerce Developers",
      "email": "developers@wellcommerce.org"
    }
  ],
  "support": {
    "email": "support@wellcommerce.org",
    "issues": "https://github.com/WellCommerce/WellCommerce/issues",
    "source": "https://github.com/WellCommerce/WellCommerce/"
  },
  "autoload": {
    "psr-4": {
      "WellCommerce\\Bundle\\ApiBundle\\": "src/WellCommerce/Bundle/ApiBundle",
      "WellCommerce\\Bundle\\AppBundle\\": "src/WellCommerce/Bundle/AppBundle",
      "WellCommerce\\Bundle\\CatalogBundle\\": "src/WellCommerce/Bundle/CatalogBundle",
      "WellCommerce\\Bundle\\CmsBundle\\": "src/WellCommerce/Bundle/CmsBundle",
      "WellCommerce\\Bundle\\CoreBundle\\": "src/WellCommerce/Bundle/CoreBundle",
      "WellCommerce\\Bundle\\CouponBundle\\": "src/WellCommerce/Bundle/CouponBundle",
      "WellCommerce\\Bundle\\GeneratorBundle\\": "src/WellCommerce/Bundle/GeneratorBundle",
      "WellCommerce\\Bundle\\OAuthBundle\\": "src/WellCommerce/Bundle/OAuthBundle",
      "WellCommerce\\Bundle\\OrderBundle\\": "src/WellCommerce/Bundle/OrderBundle",
      "WellCommerce\\Bundle\\ReviewBundle\\": "src/WellCommerce/Bundle/ReviewBundle",
      "WellCommerce\\Bundle\\RoutingBundle\\": "src/WellCommerce/Bundle/RoutingBundle",
      "WellCommerce\\Bundle\\SearchBundle\\": "src/WellCommerce/Bundle/SearchBundle",
      "WellCommerce\\Bundle\\ShowcaseBundle\\": "src/WellCommerce/Bundle/ShowcaseBundle",
      "WellCommerce\\Bundle\\WishlistBundle\\": "src/WellCommerce/Bundle/WishlistBundle",
      "WellCommerce\\Component\\Breadcrumb\\": "src/WellCommerce/Component/Breadcrumb",
      "WellCommerce\\Component\\Collections\\": "src/WellCommerce/Component/Collections",
      "WellCommerce\\Component\\DataGrid\\": "src/WellCommerce/Component/DataGrid",
      "WellCommerce\\Component\\DataSet\\": "src/WellCommerce/Component/DataSet",
      "WellCommerce\\Component\\DoctrineEnhancer\\": "src/WellCommerce/Component/DoctrineEnhancer",
      "WellCommerce\\Component\\Form\\": "src/WellCommerce/Component/Form",
      "WellCommerce\\Component\\Search\\": "src/WellCommerce/Component/Search",
      "WellCommerce\\Component\\Serializer\\": "src/WellCommerce/Component/Serializer"
    },
    "files": [
      "app/AppKernel.php"
    ]
  },
  "require": {
    "php": "^7.0",
    "ext-mbstring": "*",
    "ext-gd": "*",
    "ext-intl": "*",
    "ext-exif": "*",
    "wellcommerce/composer-plugin": "^1.0@dev",
    "wellcommerce/api-bundle": "^0.7.0@dev",
    "wellcommerce/app-bundle": "^0.7.0@dev",
    "wellcommerce/catalog-bundle": "^0.7.0@dev",
    "wellcommerce/cms-bundle": "^0.7.0@dev",
    "wellcommerce/core-bundle": "^0.7.0@dev",
    "wellcommerce/coupon-bundle": "^0.7.0@dev",
    "wellcommerce/generator-bundle": "^0.7.0@dev",
    "wellcommerce/oauth-bundle": "^0.7.0@dev",
    "wellcommerce/order-bundle": "^0.7.0@dev",
    "wellcommerce/review-bundle": "^0.7.0@dev",
    "wellcommerce/routing-bundle": "^0.7.0@dev",
    "wellcommerce/search-bundle": "^0.7.0@dev",
    "wellcommerce/showcase-bundle": "^0.7.0@dev",
    "wellcommerce/wishlist-bundle": "^0.7.0@dev"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^1.0"
  },
  "scripts": {
    "pre-install-cmd": [
      "WellCommerce\\Composer\\HerokuEnvironment::populateEnvironment"
    ],
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
    ],
    "build-bootstrap": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
    ]
  },
  "config": {
    "bin-dir": "bin",
    "secure-http": false
  },
  "extra": {
    "wellcommerce-developer-mode": false,
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml",
      "env-map": {
        "database_name": "SYMFONY_DATABASE_NAME",
        "database_user": "SYMFONY_DATABASE_USER",
        "database_host": "SYMFONY_DATABASE_HOST",
        "database_password": "SYMFONY_DATABASE_PASSWORD",
        "prod_log_path": "SYMFONY_PROD_LOG_PATH"
      }
    }
  }
}
