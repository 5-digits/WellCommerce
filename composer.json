{
  "name": "wellcommerce/standard-edition-bundle",
  "type": "wellcommerce-bundle",
  "license": "MIT",
  "minimum-stability": "dev",
  "description": "WellCommerce Standard Edition Bundle",
  "keywords": [
    "wellcommerce",
    "e-commerce",
    "open-source",
    "symfony"
  ],
  "authors": [
    {
      "name": "Adam Piotrowski",
      "email": "adam@wellcommerce.org"
    },
    {
      "name": "WellCommerce Developers",
