<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <import resource="routing/channel.xml"/>
    <import resource="routing/currency.xml"/>
    <import resource="routing/locale.xml"/>
    <import resource="routing/dictionary.xml"/>
    <import resource="routing/package.xml"/>
    <import resource="routing/routing.xml"/>

    <route id="front.home_page.index" path="/">
        <default key="_controller">web.home_page.controller.front:indexAction</default>
    </route>
</routes>
