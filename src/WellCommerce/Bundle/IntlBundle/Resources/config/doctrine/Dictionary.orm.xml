<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity repository-class="WellCommerce\Bundle\IntlBundle\Repository\DictionaryRepository"
            name="WellCommerce\Bundle\IntlBundle\Entity\Dictionary" table="dictionary">
        <unique-constraints>
            <unique-constraint name="dictionary_unique_idx" columns="identifier"/>
        </unique-constraints>
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="identifier" type="string" column="identifier" length="255" nullable="false"/>
    </entity>
</doctrine-mapping>
