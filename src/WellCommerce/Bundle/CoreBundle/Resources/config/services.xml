<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <imports>
        <import resource="services/helpers.xml"/>
        <import resource="services/form.xml"/>
        <import resource="services/dataset.xml"/>
        <import resource="services/datagrid.xml"/>
        <import resource="services/twig.xml"/>
    </imports>
    <services>
        <service id="configuration_convert.command" class="WellCommerce\Bundle\CoreBundle\Command\ConvertConfigurationCommand">
            <tag name="console.command"/>
        </service>
    </services>
</container>
