<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="form.builder" class="WellCommerce\Bundle\CoreBundle\Form\Builder\FormBuilder">
            <argument type="service" id="form.resolver.factory"/>
            <argument type="service" id="event_dispatcher"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="form.formatter.javascript" class="WellCommerce\Bundle\CoreBundle\Form\Formatter\JavascriptFormatter" />

        <service id="form.renderer.chain" class="WellCommerce\Bundle\CoreBundle\Form\Renderer\FormRendererChain">
            <argument type="collection">
                <argument type="service" id="form.renderer.javascript"/>
            </argument>
        </service>

        <service id="form.renderer.javascript" class="WellCommerce\Bundle\CoreBundle\Form\Renderer\JavascriptRenderer">
            <argument type="service" id="form.formatter.javascript"/>
            <argument>WellCommerceAdminBundle:Form:form.html.twig</argument>
        </service>

        <service id="form.data_mapper" class="WellCommerce\Bundle\CoreBundle\Form\DataMapper\DataMapper" />

        <service id="form.request_handler" class="WellCommerce\Bundle\CoreBundle\Form\Request\RequestHandler">
            <argument type="service" id="request_stack"/>
        </service>

        <service id="form.validator" class="WellCommerce\Bundle\CoreBundle\Form\Validator\Validator" />

        <service id="form.resolver.factory" class="WellCommerce\Bundle\CoreBundle\Form\Resolver\FormResolverFactory" />

        <service id="form.resolver.element" class="WellCommerce\Bundle\CoreBundle\Form\Resolver\ElementResolver">
            <tag name="form.resolver" type="element"/>
        </service>

        <service id="form.resolver.filter" class="WellCommerce\Bundle\CoreBundle\Form\Resolver\FilterResolver">
            <tag name="form.resolver" type="filter"/>
        </service>

        <service id="form.resolver.rule" class="WellCommerce\Bundle\CoreBundle\Form\Resolver\RuleResolver">
            <tag name="form.resolver" type="rule"/>
        </service>

        <service id="form.resolver.dependency" class="WellCommerce\Bundle\CoreBundle\Form\Resolver\DependencyResolver">
            <tag name="form.resolver" type="dependency"/>
        </service>

        <service id="form.element.form" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Form" scope="prototype">
            <tag name="form.element" alias="form"/>
        </service>

        <service id="form.element.attribute_editor" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Editor\AttributeEditor" scope="prototype">
            <tag name="form.element" alias="attribute_editor"/>
        </service>

        <service id="form.element.text_field" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Input\TextField" scope="prototype">
            <tag name="form.element" alias="text_field"/>
        </service>

        <service id="form.element.slug_field" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Input\SlugField" scope="prototype">
            <tag name="form.element" alias="slug_field"/>
        </service>

        <service id="form.element.date" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Input\Date" scope="prototype">
            <tag name="form.element" alias="date"/>
        </service>

        <service id="form.element.font_style" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Input\FontStyle" scope="prototype">
            <tag name="form.element" alias="font_style"/>
        </service>

        <service id="form.element.submit" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Button\Submit" scope="prototype">
            <tag name="form.element" alias="submit"/>
        </service>

        <service id="form.element.price" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Editor\PriceEditor" scope="prototype">
            <tag name="form.element" alias="price_editor"/>
        </service>

        <service id="form.element.product_variants_editor" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Editor\ProductVariantsEditor" scope="prototype">
            <tag name="form.element" alias="product_variants_editor"/>
        </service>

        <service id="form.element.password" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Input\Password" scope="prototype">
            <tag name="form.element" alias="password"/>
        </service>

        <service id="form.element.tip" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Fixed\Tip" scope="prototype">
            <tag name="form.element" alias="tip"/>
        </service>

        <service id="form.element.rich_text_editor" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Input\RichTextEditor" scope="prototype">
            <tag name="form.element" alias="rich_text_editor"/>
        </service>

        <service id="form.element.text_area" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Input\TextArea" scope="prototype">
            <tag name="form.element" alias="text_area"/>
        </service>

        <service id="form.element.tree" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Tree\Tree" scope="prototype">
            <tag name="form.element" alias="tree"/>
        </service>

        <service id="form.element.checkbox" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Input\Checkbox" scope="prototype">
            <tag name="form.element" alias="checkbox"/>
        </service>

        <service id="form.element.select" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Optioned\Select" scope="prototype">
            <tag name="form.element" alias="select"/>
        </service>

        <service id="form.element.multi_select" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Optioned\MultiSelect" scope="prototype">
            <tag name="form.element" alias="multi_select"/>
        </service>

        <service id="form.element.fieldset" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Fieldset\NestedFieldset" scope="prototype">
            <tag name="form.element" alias="nested_fieldset"/>
        </service>

        <service id="form.element.fieldset_language" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Fieldset\LanguageFieldset" scope="prototype">
            <tag name="form.element" alias="language_fieldset"/>
        </service>

        <service id="form.element.fieldset_repeatable" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Fieldset\RepeatableFieldset" scope="prototype">
            <tag name="form.element" alias="repeatable_fieldset"/>
        </service>

        <service id="form.element.layout_boxes_list" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Editor\LayoutBoxesList" scope="prototype">
            <tag name="form.element" alias="layout_boxes_list"/>
        </service>

        <service id="form.element.image" class="WellCommerce\Bundle\CoreBundle\Form\Elements\Upload\Image" scope="prototype">
            <tag name="form.element" alias="image"/>
        </service>

        <service id="form.filter.no_code" class="WellCommerce\Bundle\CoreBundle\Form\Filters\NoCode" scope="prototype">
            <tag name="form.filter" alias="no_code"/>
        </service>

        <service id="form.filter.secure" class="WellCommerce\Bundle\CoreBundle\Form\Filters\Secure" scope="prototype">
            <tag name="form.filter" alias="secure"/>
        </service>

        <service id="form.filter.trim" class="WellCommerce\Bundle\CoreBundle\Form\Filters\Trim" scope="prototype">
            <tag name="form.filter" alias="trim"/>
        </service>

        <service id="form.filter.comma_to_dot_changer" class="WellCommerce\Bundle\CoreBundle\Form\Filters\CommaToDotChanger" scope="prototype">
            <tag name="form.filter" alias="comma_to_dot_changer"/>
        </service>

        <service id="form.rule.compare" class="WellCommerce\Bundle\CoreBundle\Form\Rules\Compare" scope="prototype">
            <tag name="form.rule" alias="compare"/>
        </service>

        <service id="form.rule.custom" class="WellCommerce\Bundle\CoreBundle\Form\Rules\Custom" scope="prototype">
            <tag name="form.rule" alias="custom"/>
        </service>

        <service id="form.rule.date_to" class="WellCommerce\Bundle\CoreBundle\Form\Rules\DateTo" scope="prototype">
            <tag name="form.rule" alias="date_to"/>
        </service>

        <service id="form.rule.email" class="WellCommerce\Bundle\CoreBundle\Form\Rules\Email" scope="prototype">
            <tag name="form.rule" alias="email"/>
        </service>

        <service id="form.rule.format" class="WellCommerce\Bundle\CoreBundle\Form\Rules\Format" scope="prototype">
            <tag name="form.rule" alias="format"/>
        </service>

        <service id="form.rule.language_unique" class="WellCommerce\Bundle\CoreBundle\Form\Rules\LanguageUnique" scope="prototype">
            <tag name="form.rule" alias="language_unique"/>
        </service>

        <service id="form.rule.required" class="WellCommerce\Bundle\CoreBundle\Form\Rules\Required" scope="prototype">
            <tag name="form.rule" alias="required"/>
        </service>

        <service id="form.rule.unique" class="WellCommerce\Bundle\CoreBundle\Form\Rules\Unique" scope="prototype">
            <tag name="form.rule" alias="unique"/>
        </service>

        <service id="form.dependency.show" class="WellCommerce\Bundle\CoreBundle\Form\Dependencies\Show" scope="prototype">
            <tag name="form.dependency" alias="show"/>
        </service>
    </services>
</container>
