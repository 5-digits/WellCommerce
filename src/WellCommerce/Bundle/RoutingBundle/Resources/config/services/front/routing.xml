<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="router.provider.endpoint" class="WellCommerce\Bundle\AppBundle\Provider\RouteProvider">
            <argument type="service" id="route.generator.collection" />
            <argument type="service" id="routing.repository" />
        </service>

        <service id="route.generator.collection" class="WellCommerce\Bundle\CoreBundle\Service\Routing\Generator\RouteGeneratorCollection" />
    </services>
</container>
