<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <imports>
        <import resource="admin/order_status.xml"/>
    </imports>

    <services>
        <service id="order_status.manager" class="WellCommerce\Bundle\OrderBundle\Manager\OrderStatusManager" parent="core.manager">
            <argument type="service" id="order_status.repository"/>
            <argument type="service" id="order_status.factory"/>
        </service>
    </services>
</container>
