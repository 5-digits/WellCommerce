<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <imports>
        <import resource="admin/product_search.xml"/>
        <import resource="front/product_search.xml"/>
    </imports>

    <services>
        <service id="product_search.event_dispatcher" class="WellCommerce\Bundle\ProductBundle\EventDispatcher\ProductSearchEventDispatcher">
            <argument type="service" id="event_dispatcher" />
        </service>
    </services>
</container>
