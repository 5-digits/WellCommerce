{% if dataset.pagination.totalRows > 0 %}
    {% if app.request.get('view') == 'list' %}
        {% include 'WellCommerceCatalogBundle:Front/Common:products_list.html.twig' %}
    {% else %}
        {% include 'WellCommerceCatalogBundle:Front/Common:products.html.twig' %}
    {% endif %}
{% else %}
    <div class="alert alert-info" role="alert">
        <strong>{% trans %}category.flash.empty_category{% endtrans %}</strong>
    </div>
{% endif %}
