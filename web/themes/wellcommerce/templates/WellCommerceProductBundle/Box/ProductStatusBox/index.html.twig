{% if dataset.pagination.totalRows > 0 %}
	{% if app.request.attributes.get('_route') == 'front.home_page.index' %}
	{% include 'WellCommerceProductBundle:Front/ProductStatus:products.html.twig' %}
	{% else %}
	{% include 'WellCommerceProductBundle:Front/Common:products.html.twig' %}
	{% endif %}

{% else %}
    <div class="alert alert-info" role="alert">
        <strong>{% trans %}category.flash.empty_category{% endtrans %}</strong>
    </div>
{% endif %}
