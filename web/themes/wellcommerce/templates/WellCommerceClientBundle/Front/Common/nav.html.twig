{% trans_default_domain "wellcommerce" %}

{% if app.user and is_granted('ROLE_CLIENT') %}
    <a href="{{ path('front.client.index') }}" class="btn btn-default btn-sm">{% trans %}client.link.my_account{% endtrans %}</a>
    <a href="{{ path('front.client.logout') }}" class="btn btn-default btn-sm">{% trans %}client.link.logout{% endtrans %}</a>
{%  else %}
    <a href="{{ path('front.client.login') }}" class="btn btn-default btn-sm">{% trans %}client.link.login{% endtrans %}</a>
{% endif %}
